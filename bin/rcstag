# tag everything from . with the rev on $1

if ($#argv != 1) goto usage
if ("$1" =~ -*) goto usage

rcstree -d rcs -q -N"$1":
exit $status

usage:
echo Purpose: set tag on everything from . down.
echo Usage: $0 '"tag"'
exit 1

# For RCS Only -- Do Not Edit
