B = $(TELHOME)/bin

install: $B/nogppp	\
	$B/nog.chat	\
	$B/pppoff	\
	$B/nogchkin

pass5: install

$B/nogppp: nogppp
	rm -f $B/nogppp
	cp nogppp $B/nogppp

$B/nog.chat: nog.chat
	rm -f $B/nog.chat
	cp nog.chat $B/nog.chat

$B/pppoff: pppoff
	rm -f $B/pppoff
	cp pppoff $B/pppoff

$B/nogchkin: nogchkin
	rm -f $B/nogchkin
	cp nogchkin $B/nogchkin

# For RCS Only -- Do Not Edit
