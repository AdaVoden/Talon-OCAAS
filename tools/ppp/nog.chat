#!/bin/sh
#
# This is used by the infippp script to dial up.
#
exec /usr/sbin/chat -v					\
	TIMEOUT		10				\
	ABORT		'\nBUSY\r'			\
	ABORT		'\nNO ANSWER\r'			\
	''		'\rAT'				\
	'OK-+++\c-OK'	ATH0				\
	TIMEOUT		60				\
	OK		ATDT$TELEPHONE			\
	ogin:		$ACCOUNT			\
	assword:	$PASSWORD			\
	beginning

